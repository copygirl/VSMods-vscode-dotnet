{
	"version": "2.0.0",
	"tasks": [{
		
		"label": "build",
		"group": { "kind": "build", "isDefault": true },
		"presentation": { "reveal": "silent" },
		"problemMatcher": "$msCompile",
		
		"type": "shell",
		"command": "dotnet",
		"linux": { "options": { "env": { "FrameworkPathOverride": "/lib/mono/4.5/" } } },
		"args": [ "build", "-c", "Debug" ]
		
	},{
		
		"label": "test",
		"group": { "kind": "test", "isDefault": true },
		
		"type": "shell",
		"command": "dotnet",
		"linux": { "options": { "env": { "FrameworkPathOverride": "/lib/mono/4.5/" } } },
		"args": [ "test" ]
		
	}]
}
