{
	"version": "0.2.0",
	"configurations": [{
		"name": "Launch Client (.NET)",
		"type": "clr",
		"request": "launch",
		"preLaunchTask": "build",
		"program": "${env:VINTAGE_STORY}/Vintagestory.exe",
		"args": [
			"-p3", "--openWorld", "modding test world.vcdbs",
			"--addOrigin", "${workspaceFolder}/GrowingTrees/assets",
		],
		"console": "internalConsole",
		"stopAtEntry": false,
		"internalConsoleOptions": "openOnSessionStart"
	},{
		"name": "Attach to Mono",
		"type": "mono",
		"request": "attach",
		"address": "localhost",
		"port": 55555
	}]
}